<script >
//鼠标滑过星星触发
$('#rank_pic img').mouseover(function(){
	//当前星星
    var str=$(this).attr('src');
	var str=str.replace('rank_3.gif','rank_1.gif');
	var str=str.replace('rank_4.gif','rank_2.gif');
	$(this).attr('src',str);
	//当前前面的星星
	$(this).prevAll().each(function(){
	   var str=$(this).attr('src');
	   var str=str.replace('rank_3.gif','rank_1.gif');
	   var str=str.replace('rank_4.gif','rank_2.gif');
	   $(this).attr('src',str);
	});

	//当前后面的星星
	$(this).nextAll().each(function(){
	   var str=$(this).attr('src');
	   var str=str.replace('rank_1.gif','rank_3.gif');
	   var str=str.replace('rank_2.gif','rank_4.gif');
	   $(this).attr('src',str);
	});
});

//鼠标单击星星出发
$('#rank_pic img').click(function(){
	var  index=$(this).prevAll().index();
	var pfz=(index+2)/2;
	//发送ajax请求,更新评分值和评分人数
	$.ajax({
		'type':'get',
		'url':'{dede:global.cfg_plus_dir/}/pingfen.php?act=update&id={dede:field.aid/}&pfz='+pfz,
		'dataType':'json',
		'success':function(msg){//返回json数据
			if(msg==-1){
                    alert('亲,一天只能评分一次哦!');
                    return false;
               }
		 $("#rank").html(msg.pfz);
         $("#rank_num").html(msg.pfrs);		
		}
	
	});

});

//页面加载后发送ajax请求,获取当前的电影的评分值和评分人数
$.ajax({
    type:'get',
    dataType:'json',
    url:'{dede:global.cfg_plus_dir/}/pingfen.php?act=getpingfen&id={dede:field.aid/}',
    success:function(msg){
               $("#rank").html(msg.pfz);
               $("#rank_num").html(msg.pfrs);
			   
            } 
});
//获取电影评论
$.ajax({
type:'get',
url:'{dede:global.cfg_plus_dir/}/getpinglun.php?id={dede:field.aid/}',
success:function(msg){
    //alert(msg);
$('#showpinglun').html(msg);

}

});

   
</script>